  "scripts": {
    "compile": "npx hardhat clean && npx hardhat compile",
    "build": "rm -r ./build; npx truffle build",
    "compile1": "npx truffle compile && cp build/contracts/ConfigController.json predeployed/config_controller_predeployed/artifacts",
    "lint": "npx ethlint -d contracts",
    "lint-fix": "npm run lint -- --fix",
    "predeployed-test": "npx truffle test test/predeployed/* --network skaled",
    "slither": "slither . --filter-path strings --filter-path Migrations",
    "version": "node -e \"console.log(require('./package.json').version);\""
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.3.2"
  },
  "devDependencies": {
      "hardhat": "^2.6.4",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
